          li
            a(href='javascript:alert("under construction")') Ameneties
          li
            a(href='javascript:alert("under construction")') Book
          li
            a(href='javascript:alert("under construction")') Contact Us
  .container
    .row
      .box
        .col-lg-12.text-center
          #carousel-example-generic.carousel.slide
            //- Indicators
            ol.carousel-indicators.hidden-xs
              li.active(data-target='#carousel-example-generic', data-slide-to='0')
              li(data-target='#carousel-example-generic', data-slide-to='1')
              li(data-target='#carousel-example-generic', data-slide-to='2')
            //- Wrapper for slides
            .carousel-inner
              .item.active
                img.img-responsive.img-full(src='img/slide1.jpg', alt='')
              .item
                img.img-responsive.img-full(src='img/slide2.jpg', alt='')
              .item
                img.img-responsive.img-full(src='img/slide3.jpg', alt='')
            //- Controls
            a.left.carousel-control(href='#carousel-example-generic', data-slide='prev')
              span.icon-prev
            a.right.carousel-control(href='#carousel-example-generic', data-slide='next')
              span.icon-next
          h2.brand-before
            small Welcome to
          h1.brand-name the 8601
